					<field name="countryName" type="STRING" />
					<field name="currencyName" type="STRING" />
				</fields>
			</source>

			<target name="test">
				<fields>
					<field name="nombre_pais" type="STRING" order="1" value="countryName" />
					<field name="nombre_moneda" type="STRING" order="2"
						value="currencyName" />
				</fields>
			</target>

		</translation>

	</translations>

